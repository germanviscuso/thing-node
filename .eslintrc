{
  "parserOptions": {
    "ecmaVersion": 5,
    "sourceType": "module"
  },
  "rules": {
    "no-var": 0,
    "prefer-arrow-callback": 0,
    "curly": 0,
    "spaced-comment": 0,
    "prefer-template": 0,
    "max-nested-callbacks": 0
  },
  "extends": "xo-space",
  "env": {
    "mocha": true
  }
}
